<%= render "layouts/errors", :object => @meal %>

<%= form_for @meal do |f| %>
<%= f.label :breakfast %>
<%= f.text_field :breakfast %>
<%= f.label :lunch %>
<%= f.text_field :lunch %>
<%= f.label :dinner %>
<%= f.text_field :dinner %>
<%= f.label :snacks %>
<%= f.text_field :snacks %>
<br><br>
<%= f.submit %>
<% end %>
